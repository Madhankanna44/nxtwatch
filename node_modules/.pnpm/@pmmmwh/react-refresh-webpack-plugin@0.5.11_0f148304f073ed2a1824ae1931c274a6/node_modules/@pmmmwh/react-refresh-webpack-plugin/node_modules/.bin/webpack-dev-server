#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nxtwatch/node_modules/.pnpm/webpack-dev-server@4.15.1_webpack@5.90.2/node_modules/webpack-dev-server/bin/webpack-dev-server.js/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/webpack-dev-server@4.15.1_webpack@5.90.2/node_modules/webpack-dev-server/bin/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/webpack-dev-server@4.15.1_webpack@5.90.2/node_modules/webpack-dev-server/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/webpack-dev-server@4.15.1_webpack@5.90.2/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/node_modules:/home/nxtwave/nxtwatch/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/@pmmmwh/react-refresh-webpack-plugin@0.5.11_0f148304f073ed2a1824ae1931c274a6/node_modules/@pmmmwh/react-refresh-webpack-plugin/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/@pmmmwh/react-refresh-webpack-plugin@0.5.11_0f148304f073ed2a1824ae1931c274a6/node_modules/@pmmmwh/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/@pmmmwh/react-refresh-webpack-plugin@0.5.11_0f148304f073ed2a1824ae1931c274a6/node_modules:/home/nxtwave/nxtwatch/node_modules/.pnpm/@pmmmwh/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../webpack-dev-server@4.15.1_webpack@5.90.2/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
else 
  exec node  "$basedir/../../../../../../../webpack-dev-server@4.15.1_webpack@5.90.2/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
